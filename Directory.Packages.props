    <PackageVersion Include="Swashbuckle.AspNetCore" Version="6.6.2" />
    <!-- Transitive, referenced by Swashbuckle.AspNetCore -->
    <PackageVersion Include="Microsoft.Extensions.ApiDescription.Server" Version="8.0.6" />
    <PackageVersion Include="Microsoft.OpenApi" Version="1.6.15" />
  </ItemGroup>

  <ItemGroup>
    <!-- BlazorWebApp -->
    <PackageVersion Include="Microsoft.AspNetCore.Components.Web" Version="8.0.6" />
    <!-- Transitive, referenced by Microsoft.AspNetCore.Components.Web -->
    <PackageVersion Include="Microsoft.AspNetCore.Components" Version="8.0.6" />
    <PackageVersion Include="Microsoft.AspNetCore.Components.Forms" Version="8.0.6" />
    <PackageVersion Include="Microsoft.Extensions.DependencyInjection" Version="8.0.0" />
    <PackageVersion Include="Microsoft.Extensions.Primitives" Version="8.0.0" />
    <PackageVersion Include="Microsoft.JSInterop" Version="8.0.6" />
    <PackageVersion Include="System.IO.Pipelines" Version="8.0.0" />

    <!-- Transitive, referenced by Microsoft.AspNetCore.Components -->
    <PackageVersion Include="Microsoft.AspNetCore.Authorization" Version="8.0.6" />
    <PackageVersion Include="Microsoft.AspNetCore.Components.Analyzers" Version="8.0.6" />

    <!-- Transitive, referenced by Microsoft.AspNetCore.Authorization -->
    <PackageVersion Include="Microsoft.AspNetCore.Metadata" Version="8.0.6" />
    <PackageVersion Include="Microsoft.Extensions.Logging.Abstractions" Version="8.0.1" />
    <PackageVersion Include="Microsoft.Extensions.Options" Version="8.0.2" />
    <!-- Transitive, referenced by Microsoft.Extensions.DependencyInjection, Microsoft.Extensions.Logging.Abstractions, Microsoft.Extensions.Options -->
    <PackageVersion Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="8.0.1" />
  </ItemGroup>

  <ItemGroup>
    <!-- Unit Testing -->
    <PackageVersion Include="coverlet.collector" Version="6.0.2" />
    <PackageVersion Include="FluentAssertions" Version="6.12.0" />
    <!-- Transitive, referenced by FluentAssertions -->
