    <PackageVersion Include="Microsoft.TestPlatform.ObjectModel" Version="17.10.0" />
    <PackageVersion Include="Newtonsoft.Json" Version="13.0.3" />
    <!-- Transitive, referenced by Microsoft.TestPlatform.ObjectModel -->
    <PackageVersion Include="NuGet.Frameworks" Version="6.10.1" />
    <PackageVersion Include="System.Collections.Immutable" Version="8.0.0" />
    <PackageVersion Include="System.Reflection.Metadata" Version="8.0.0" />

    <PackageVersion Include="xunit" Version="2.8.1" />
    <!-- Transitive, referenced by xUnit -->
    <PackageVersion Include="xunit.abstractions" Version="2.0.3" />
    <PackageVersion Include="xunit.analyzers" Version="1.14.0" />
    <PackageVersion Include="xunit.assert" Version="2.8.1" />

