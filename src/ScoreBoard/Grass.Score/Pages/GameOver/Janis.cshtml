@page
@model JanisGameOver
@{ ViewData["Title"] = "game over"; }
<head>
  <script language="javascript" type="text/javascript">var hands = 2; var winner = true;</script>
</head>

<div class="fixedHeader">
  <div class="pageHeader level mb-0 py-2 px-4">
    <div class="level-left">Game target: $250,000</div>
    <div class="level-center">Amy won</div>
    <div class="level-right game-datetime">July 1, 2024 10:59 AM</div>
  </div>
</div>

<div class="fixedContent">
  <nav id="showInPlay" class="my-1 px-2 is-hidden" role="navigation">
    <a id="inPlay" class="button" onclick="toggleInPlay()">Show in hand</a>
  </nav>
  <div class="columns pl-2 pr-2 fixedContent">
    <div id="Resizer" class="is-hidden"></div>
    <div class="column">
      <nav class="menu sticky">
        <p class="menu-label"><strong>Janis</strong></p>
        <ul id="InThisArticleMenu" class="menu-list">
          <li><a id="inHandLink" class="quickLink is-hidden">In hand</a></li>
          <li><a class="quickLink">Stash pile</a></li>
          <li><a id="scoreCardsLink" class="quickLink is-hidden">Score cards</a></li>
        </ul>
      </nav>
    </div>
    <div class="column content is-8">
      <table class="table">
        <tr>
          <td class="game-card"><img class="image" title="played (round 2)" src="https://KevinDHeath.github.io/score/media/Market-Open.png"></td>
          <td><p style="margin-bottom: 0px;">Game total: $178,000<br />Stash total: $75,000</p>
            <div style="height: 110px; overflow: auto;"><table style="height: 110px;" class="table">
              <tr><td><ul style="margin-left: 9px; margin-top: 0px;">
                <li class="hassle-history">Heat off Bust played (round 14)</li>
                <li class="hassle-history">Heat on Bust by Amy when stash was 150,000 (round 10)</li>
                <li class="hassle-history">Heat off Search and seizure played (round 8)</li>
                <li class="hassle-history">Heat on Search and seizure by Amy when stash was 125,000 (round 8)</li>
              </ul></td></tr>
            </table></div>
          </td>
        </tr>
      </table>
      <div id="inHand" class="is-hidden">
        <h2 class="quickLinkHeader"><span class="icon toggleSection"><i class="fa fa-angle-down"></i></span>In hand</h2>
        <table class="table">
          <tr>
            <td class="game-card"><img class="image" src="https://KevinDHeath.github.io/score/media/Market-Close.png"></td>
            <td class="game-card"><img class="image" title="passed by Bob (round 10)" src="https://KevinDHeath.github.io/score/media/HeatOff-Detained.png"></td>
            <td class="game-card"><img class="image" src="https://KevinDHeath.github.io/score/media/Nirvana-Stonehigh.png"></td>
            <td class="game-card"><img class="image" title="passed by Bob (round 17)" src="https://KevinDHeath.github.io/score/media/HeatOff-Felony.png"></td>
            <td class="game-card"><img class="image" src="https://KevinDHeath.github.io/score/media/Skim-TheBanker.png"></td>
            <td class="game-card"><img class="image" title="passed by Bob (round 20)" src="https://KevinDHeath.github.io/score/media/Paranoia-Soldout.png"></td>
          </tr>
        </table>
      </div>
      <h2 class="quickLinkHeader"><span class="icon toggleSection"><i class="fa fa-angle-down"></i></span>Stash pile</h2>
      <table class="table">
        <tr>
          <td class="game-card"><img class="image" title="played (round 15)" src="https://KevinDHeath.github.io/score/media/Protection-LustConquersAll.png"></td>
          <td class="game-card"><img class="image" title="played (round 4)" src="https://KevinDHeath.github.io/score/media/Protection-GrabaSnack.png"></td>
        </tr>
      </table>
      <div id="scoreCards" class="">
        <h2 class="quickLinkHeader"><span class="icon toggleSection"><i class="fa fa-angle-down"></i></span>Score cards</h2>
        <div class="columns is-multiline">
          <div class="column">
            <table class="score-card">
              <tr><th colspan='2'>2nd Hand</th></tr>
              <tr><td>Protected profit</td><td class="align-right">75,000</td></tr>
              <tr><td>+ At risk profit</td><td class="align-right">0</td></tr>
              <tr><td>- Banker's skim /+ bonus</td><td class="align-right">84,000</td></tr>
              <tr><td>- Highest Peddle in hand</td><td class="align-right">0</td></tr>
              <tr><td>- Paranoia</td><td class="align-right"><span class="negative-value">25,000</span></td></tr>
              <tr><td>= Net profit</td><td class="align-right">134,000</td></tr>
              <tr><td nowrap>+ Bonus for winner of hand</td><td class='align-right'>0</td></tr>
              <tr><td>Total</td><td class="align-right">134,000</td></tr>
            </table>
          </div>
          <div class="column">
            <table class="score-card">
              <tr><th colspan='2'>1st Hand</th></tr>
              <tr><td>Protected profit</td><td class="align-right">0</td></tr>
              <tr><td>+ At risk profit</td><td class="align-right">180,000</td></tr>
              <tr><td>- Banker's skim /+ bonus</td><td class="align-right"><span class="negative-value">36,000</span></td></tr>
              <tr><td>- Highest Peddle in hand</td><td class="align-right">0</td></tr>
              <tr><td>- Paranoia</td><td class="align-right"><span class="negative-value">100,000</span></td></tr>
              <tr><td>= Net profit</td><td class="align-right">44,000</td></tr>
              <tr><td nowrap>+ Bonus for winner of hand</td><td class='align-right'>0</td></tr>
              <tr><td>Total</td><td class="align-right">44,000</td></tr>
            </table>
          </div>
        </div>
      </div>
    </div>
    <div class="column">
      <nav class="menu sticky">
        <p class="menu-label"><strong>Players</strong></p>
        <ul class="toc-menu">
          <li><a asp-area="" asp-page="/GameOver/Amy">Amy</a></li>
          <li><a asp-area="" asp-page="/GameOver/Bob">Bob</a></li>
          <li><a class="player-current">Janis</a></li>
          <li><a asp-area="" asp-page="/GameOver/John">John</a></li>
        </ul>
      </nav>
    </div>
  </div>
</div>

<script>
  $(function () {
    initializePage();
    InitializeQuickLinks();

    $(".toggleSection").click(function () {
      SectionExpandCollapse(this);
    });

    $(".toggleSection").keypress(function () {
      SectionExpandCollapseCheckKey(this, event)
    });
  });
</script>